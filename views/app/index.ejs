<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head.ejs') -%>
</head>
<body>
    
   <header>
       <h1> Welcome to Biblioteca</h1>

       <h2> Catalogue</h2>
   </header>

   <main> 
    <ul> 
        <% Books.forEach(element => { %> 
            <li>    
            <a href="/<%=element._id%>">
            <%=element.title%> <br> 
            </a>
            <%=element.author%>  <br> 
            <%=element._id%> 
            </li>
        <%})%>
        </ul> 
    </main>


    <footer> 

    <a href="/new"> Add new book</a>

    </footer>

</body>
</html>